@model IEnumerable<BARG.Models.Assessment>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Assessments</h2>

<table class="table">
    <thead>
        <tr>
            <th>-</th>
            <th>-</th>

            <th>-</th>

            <th>-</th>

        </tr>

    </thead>
    <body>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>

            </tr>
        }

    </body>
</table>


}
